                <div class="col-12">
                <!-- This is the line that matters for import -->
                <%- include('./ai2html-output/solution.ejs'); %>
                </div>
            </div>
            </div>
        </section>

        <!--  6th static -->
        <section class="ai2html">
            <div class="container-fluid">
            <div class="row">
                <div class="col-12">
                <!-- This is the line that matters for import -->
                <%- include('./ai2html-output/name.ejs'); %>
                </div>
            </div>
            </div>
        </section>

</body>
</html>